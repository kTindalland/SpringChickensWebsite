@model SpringChickens.ViewModels.Admin.CalendarManagementViewModel;
@{
    ViewData["Title"] = "Calendar Management";
    ViewData["ShowOutdated"] = Model.ShowOutdatedEvents;
    ViewData["routeController"] = "Admin";
}


    <div class="row">
        <div class="col-md-2">

        </div>
        <div class="col-md-4 border-right">
            <a class="btn btn-success mb-3" asp-controller="Admin" asp-action="CreateNewEvent">Create Event</a>
            <partial name="_CalendarPartial" model="Model.CalendarVM" view-data="ViewData" />
        </div>
        <div class="col-md-4">
            <partial name="_CalendarEventListPartial" model="Model.AllCalendarEvents" view-data="ViewData" />
        </div>
    </div>

    
